<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.media.MediaPlayer?>
<?import javafx.scene.media.Media?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.beatrix.lab.PreloaderController" stylesheets="@../assets/main.css"
            prefHeight="500.0" prefWidth="900.0" fx:id="preloaderRoot" styleClass="preloaderRoot">

    <VBox spacing="0">
        <padding>
            <Insets top="0" left="-5" right="0" bottom="0"/>
        </padding>
        <children>
            <MediaView fx:id="mediaView" styleClass="m" pickOnBounds="true"/>
            <JFXProgressBar fx:id="progressBar" maxWidth="Infinity">
                <padding>
                    <Insets top="-7"/>
                </padding>
            </JFXProgressBar>
        </children>

    </VBox>
</AnchorPane>
